local Section = Tab:CreateSection("Button Title:")
local Section = Tab:CreateSection("")

local Tab = Window:CreateTab("others", 4483345998) -- Title, Image

local Button = Tab:CreateButton({
	Name = "Anti afk",
	Callback = function()
		repeat wait() until game:IsLoaded()
				game:GetService("Players").LocalPlayer.Idled:connect(function()
				game:GetService("VirtualUser"):ClickButton2(Vector2.new())
				local vu = game:GetService("VirtualUser")
		end)
		game:GetService("Players").LocalPlayer.Idled:connect(function()
			 vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
			 wait(1)
			 vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
		game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage", {
	 						Text = "[VISUALHUB]: "..(notifi).."",
	 						Color = Color3.fromRGB(0, 255, 0),
	 						Font = Enum.Font.GothamBold,
	 						TextSize = 16,
	 				})
		end)
	end,
})

local Tab = Window:CreateTab("Chat", 4483345998) -- Title, Image

local Button = Tab:CreateButton({
	Name = "ClearChat",
	Callback = function()
		game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage", {
						Text = "[VISUALHUB]: Clear Chat Activated",
						Color = Color3.fromRGB(0, 255, 0),
						Font = Enum.Font.GothamBold,
						TextSize = 16,
				})

	loadstring(game:HttpGet("https://raw.githubusercontent.com/VateqS/Visual0Hub/main/ClearChat"))()
	
	game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage", {
					Text = "[VISUALHUB]: Chat Succesfully Cleared",
					Color = Color3.fromRGB(0, 255, 0),
					Font = Enum.Font.GothamBold,
					TextSize = 16,
			})
	end,
})

