local Rayfield = loadstring(game:HttpGet("https://raw.githubusercontent.com/shlexware/Rayfield/main/source"))()

local Window = Rayfield:CreateWindow({
	Name = "VisualHub",
	LoadingTitle = "VisualHub",
	LoadingSubtitle = "by VateqS",
	ConfigurationSaving = {
		Enabled = false,
		FolderName = nil, -- Create a custom folder for your hub/game
		FileName = "VisualHub"
	},
        Discord = {
        	Enabled = true,
        	Invite = "qF38fGz8dr", -- The Discord invite code, do not include discord.gg/
        	RememberJoins = true -- Set this to false to make them join the discord every time they load it up
        },
	KeySystem = true, -- Set this to true to use our key system
	KeySettings = {
		Title = "KeySystem",
		Subtitle = "(discord.gg/qF38fGz8dr)",
		Note = "(discord.gg/qF38fGz8dr)",
		FileName = "",
		SaveKey = true,
		GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
		Key = "Visual2759127973612938576"
	}
})

local Tab = Window:CreateTab("Language", 4483362458)

local Button = Tab:CreateButton({
	Name = "English",
	Callback = function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/VateqS/Visual0Hub/main/BFVHUB"))()
	end,
})
