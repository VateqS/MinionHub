local page = venyx:addPage("AutoFarm", 5012544693)
local section1 = page:addSection("AutoFarming")

getgenv().autofarm = false
getgenv().autofarm_area = "Area1"
getgenv().autofarm_boss = 1

section1:addDropdown("Select Area", {"Area1", "Area2", "Area3", "Area4"}, function(cas)
    getgenv().autofarm_area = tostring(cas)
end)

section1:addDropdown("Select Boss", {1,2,3,4,5}, function(cas)
    getgenv().autofarm_boss = tonumber(cas)
end)

section1:addToggle("Start Auto Farm", nil, function(value)
    getgenv().autofarm = value
    while getgenv().autofarm and wait(0.525) do
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("FightOver"):FireServer(getgenv().autofarm_area, getgenv().autofarm_boss)
    end
end)

getgenv().gui_toggle = true

local GuiService = game:GetService("GuiService")
local player = game.Players.LocalPlayer
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "GGUI_togglar"
screenGui.Parent = player.PlayerGui
local frame = Instance.new("Frame")
frame.Name = "Togglar1"
frame.Size = UDim2.new(0, 32, 0, 32)
frame.Position = UDim2.new(0.5, 430, 0, -32)
frame.BackgroundTransparency = 0.5
frame.BackgroundColor3 = Color3.new(0, 0, 0)
frame.BorderSizePixel = 0
frame.Parent = screenGui
local button = Instance.new("TextButton")
button.Text = "-"
button.TextSize = 20
button.TextStrokeTransparency = 0
button.Size = UDim2.new(1, 0, 1, 0)
button.SizeConstraint = Enum.SizeConstraint.RelativeYY
button.Position = UDim2.new(0, 0, 0, 0)
button.BackgroundTransparency = 1
button.Parent = frame
local uiCorner = Instance.new("UICorner")
uiCorner.CornerRadius = UDim.new(0, 7.5)
uiCorner.Parent = frame
button.TextColor3 = Color3.new(1, 1, 0)
button.MouseButton1Click:Connect(function()
    if getgenv().gui_toggle == false then
        getgenv().gui_toggle = true
        button.Text = "-"
        button.TextSize = 20
        button.TextColor3 = Color3.new(1, 1, 0)
        game.CoreGui["Louv Hub"].Enabled = getgenv().gui_toggle
    elseif getgenv().gui_toggle == true then
        getgenv().gui_toggle = false
        button.Text = "+"
        button.TextSize = 20
        button.TextColor3 = Color3.new(0, 1, 0)
        game.CoreGui["Louv Hub"].Enabled = getgenv().gui_toggle
    end
end)
button.MouseButton2Click:Connect(function()
    screenGui:Destroy()
    game.CoreGui["Louv Hub"]:Destroy()
end)
